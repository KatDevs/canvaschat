<div>
	<h5 class="grey-text text-darken-4 margin-ends">Line Settings</h5>
	<div class="row">
		<div class="range-field col s6" >	    	
	    	<input type="range" id="lineWidth" value="5" min="1" max="100" />
	    	<label for="lineWidth">Line Width</label>
	    </div>
	    <div class="range-field col s6" >	    	
	    	<input type="range" id="shadowWidth" value="0" min="0" max="25" />
	    	<label for="shadowWidth">Shadow Width</label>
	    </div>
	</div> 
	<div class="row">
		<div class="range-field col s4" >	    	
	    	<input type="range" id="js-red" value="0" min="0" max="255" />
	    	<label for="js-red">Red</label>
	    </div>
	    <div class="range-field col s4" >	    	
	    	<input type="range" id="js-blue" value="0" min="0" max="255" />
	    	<label for="js-blue">Blue</label>
	    </div>
	    <div class="range-field col s4" >	    	
	    	<input type="range" id="js-green" value="0" min="0" max="255" />
	    	<label for="js-green">Green</label>
	    </div>
	</div>
</div>
